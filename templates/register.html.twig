<body>
	<!-- Page Preloder -->
	<div id="preloder">
		<div class="loader"></div>
	</div>

	<!-- Header Section -->
<header>
		<div class="header__warp">
			<div class="row">
				<div class="col-lg-2">
					<a href="/" class="site__logo">
						<img src="img/logo.png" alt="">
					</a>
				</div>
			</div>
		</div>
	</header>     
	<!-- Header Section end -->
	<!-- Footer Section -->
	<footer class="footer__section">
		<div class="container">
			<div class="footer-top">
				<div class="row m-0">
					<div class="col-lg-8 p-0">
						<div class="footer-form-warp">
							<div class="section-title">
								<div class="sub-title">Register</div>
							</div>
							<form method="post" action="/login">
								<div class="row">
									<div class="col-lg-6">
										<input type="text" placeholder="Username">
									</div>
									<div class="col-lg-6">
										<input type="text" placeholder="Password">
									</div>
									<div class="col-lg-12">
                                    <input type="submit" name="submit" value="Register" class="site__btn">
									</div>
								</div>
							</form>
						</div>
					</div>
					<div class="col-lg-4 p-0">
						<img src="img/login_register.png" alt="clouds">
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-4 col-md-6">
					<div class="info__box">
						<div class="info_icon">
							<i class="fa fa-map-marker"></i>
						</div>
						<p>333 Middle Winchendon Rd, <br> Rindge, NH 03461 </p>
					</div>
				</div>
				<div class="col-lg-4 col-md-6">
					<div class="info__box info_box_center">
						<div class="info_icon">
							<i class="fa fa-map-marker"></i>
						</div>
						<p>125-711-811 | 125-668-886  <br> Support.music@gmail.com</p>
					</div>
				</div>
				<div class="col-lg-4">
					<div class="footer__social">
						<a href="#"><i class="fa fa-facebook"></i></a>
						<a href="#"><i class="fa fa-twitter"></i></a>
						<a href="#"><i class="fa fa-youtube-play"></i></a>
						<a href="#"><i class="fa fa-instagram"></i></a>
					</div>
				</div>
			</div>
